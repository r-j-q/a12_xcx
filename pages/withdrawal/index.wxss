.withdrawal {
    width: 100%;
    height: 100%;
    overflow: hidden;
    background-color: #fff;
}

.withdrawal-header {
    width: 100%;
    height: 300rpx;
    background-color: #E72CD1;
    color: #fff;
    padding-left: 40rpx;
    position: relative;
}

.withdrawal-content {
    width: 100%;
    background-color: #fff;
    border-radius: 30rpx;
    margin: auto;
}

.van-field__label {
    font-size: 30rpx !important;
    color: #333333 !important;
    font-weight: 400 !important;
}

.van-field__control {
    margin-left: 40rpx !important;
    font-size: 30rpx;
}

.withdrawal-blance {
    padding-top: 80rpx;
    padding-bottom: 40rpx;
}

.withdrawal-blance .van-field__control {
    font-size: 44rpx;
    font-weight: 600;
}

.withdrawal-after .van-field__control:after {
    content: "å…ƒ";
    color: #cccccc;
    font-size: 30rpx;
    position: absolute;
    right: 20rpx;
    top: 0;
}
.title {
    font-weight: 400;
    color: #E72CD1;
    margin: 30rpx;
    font-size: 20rpx;
}
.withdrawal-button {
    width: 90%;
    height: 100rpx;
    background: #E72CD1;
    border-radius: 10rpx;
    color: #fff;
    font-size: 32rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: auto;
    margin-top: 100rpx;
}

.blance {
    padding-top: 50rpx;
    font-size: 24rpx;
}
.price {
    margin-top: 10rpx;
    font-size: 60rpx;
    font-weight: 600;
}
.blance-title {
    font-size: 34rpx;
    font-weight: 600;
    width: 92%;
    height: 90rpx;
    display: flex;
    align-items: center;
    margin: auto;
    color: #333333;
}