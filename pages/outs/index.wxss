@import '../AboutOrder/index.wxss';


.outs {
    width: 100%;
    height: 100%;
    position: relative;
    overflow-x: hidden;
    background: linear-gradient(180deg, #FCF0FF 0%, #F4EEFF 100%)
}

.top {
    width: 100%;
    height: 120rpx;
    background-color: #fff;
}

.home-select {
    width: 100%;
    height: 100rpx;
    background: #FCF0FF;
    position: fixed;
    top: 0;
    z-index: 11;
    margin-top: 0;
    display: flex;
    align-items: center;
}

.result {
    width: 104rpx;
    height: 74rpx;
    background: #E134D9;
    border-radius: 12px 12px 12px 12px;
    position: absolute;
    right: 20rpx;
    font-weight: bold;
    color: #FFFFFF;
    display: flex;
    align-items: center;
    justify-content: center;
}

.list {
    width: 90%;
    margin: auto;
    background-color: #fff;
    margin-bottom: 20rpx;
    border-radius: 10rpx;
}

.pk {
    width: 100%;
    height: 180rpx;
    display: flex;
    align-items: center;
    position: relative;
}

.pk image {
    width: 80rpx;
    height: 80rpx;
    margin-left: 30rpx;
}

.row {
    margin-left: 20rpx;
    width: 100%;
    font-size: 30rpx;
    color: #333333;
}

.row view {
    position: relative;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.co {
    position: absolute;
    right: 40rpx;

    color: #E134D9;
}

.pk.active {
    background-color: rgba(255, 203, 236, 5)
}

.duxiain {
    width: 150rpx;
    height: 40rpx;
    background-color: #fff;
    position: absolute;
    top: 0;
    color: #E134D9;
    text-align: center;
}

.area_num {
    background-color: #9F00B6;
    height: 40rpx;
    font-size: 20rpx;
    position: absolute;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0 10rpx 0 10rpx;
    border-radius: 0rpx 20rpx 0 0;
    top: 0;
    right: 0;
    color: #fff;
    text-align: center;
}

.anniu {
    width: 100%;
    padding: 10rpx 0 10rpx 0;
    background-color: #fff;
    display: flex;
    align-items: center;
}

.anniu view {
    padding: 0 15rpx 0 15rpx;
    height: 60rpx;
    margin-left: 10rpx;
    background-color: #E72CD1;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50rpx;
}

.block {
    width: 90%;

    background-color: #FCF0FF;
    border-radius: 10rpx;

}

.names {
    width: 80%;
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 10rpx;
    font-size: 34rpx;
    font-weight: bold;
    margin: auto;
    margin-top: 20rpx;
    background-color: #fff;
    border: 1px solid #E134D9;
}

.names text {
    margin-left: 30rpx;
}

input {
    width: 180rpx;
    background-color: #fff;
    height: 180rpx;
    border-radius: 180rpx;
    text-align: center;
    margin: auto;
    margin-top: 30rpx;
}

.bifen {
    margin-top: 100rpx;
    margin-bottom: 100rpx;
}

.sure {
    width: 92%;
    height: 90rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 80rpx;
    margin: auto;
    margin-top: 80rpx;
    margin-bottom: 40rpx;
    font-size: 34rpx;
    color: #FFFFFF;
    background: linear-gradient(180deg, #9F00B6 0%, #FF4BBD 100%);
}

.cancel {
    color: #FF4BBD;
    font-size: 30rpx;
    text-align: center;
    margin-bottom: 40rpx;
}

.home-flex {
    display: flex;
    overflow-y: auto;
    width: 600rpx;
}

.home-flex view {
    width: 400rpx !important;
}
.home-cell{
    width: 80%;
    overflow: scroll;
    display: flex;
}
.home-cell view{
    width: 120rpx;
    flex: none;
}