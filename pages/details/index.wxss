.order {
    width: 100%;
    height: 100%;
    position: relative;
    overflow-x: hidden;
    background: #FCF0FF;
}
.center{
    width: 80%;
    height: 70%;
    background-color: #fff;
    border-radius: 10rpx;
    margin: auto;
}
.move-top{
    width: 100%;
    height: 15%;
    background-color: #E72CD1;
    font-size: 40rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #fff;
    font-weight: 600;
    border-radius: 10rpx 10rpx 0 0;
}
.move-list{
    width: 100%;
    height: 82%;
    overflow-y: scroll;
}
.move-cell{
    display: flex;
    width: 96%;
    margin: auto;
    margin-top: 10rpx;

    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #eee;
    align-items: center;
}
.move-cell image{
    width: 100rpx;
    height: 100rpx;
    border-radius: 100rpx;
   margin-right: 20rpx;
}
.name{
    font-size: 30rpx;
}
.member{
    color: #9F00B6;
    margin-left: 30rpx;
}
.order-top {
    width: 92%;
    height: 100rpx;
    display: flex;
    position: relative;
    margin: auto;
    font-size: 20rpx;
    color: #333333;
    margin-top: 10rpx;
    align-items: center;

}

.order-top .laba {
    width: 30rpx;
    height: 30rpx;
    z-index: 1;
    margin-left: 20rpx;
    margin-right: 10rpx;

}
.hao{
    width: 89%;
    margin: auto;
    margin-top: 40rpx;
    display: flex;
    padding-bottom: 20rpx;
}
.order-top .back {
    width: 100%;
    position: absolute;
}
.order-img{
    margin: auto;
    display: flex;
    width: 90%;
    height: 280rpx;
}
.order-center {
    width: 92%;
    background: #fff;
    margin: auto;
    margin-top: 20rpx;
    border-radius: 10rpx;
}

.order-center .rows {
    display: flex;
    width: 90%;
    margin: auto;
    padding: 30rpx 0 30rpx 0;
    align-items: center;
    border-bottom: 1rpx solid #eeeeee;
    position: relative;
}

.rows image {
    width: 70rpx;
    height: 70rpx;
}

.rows .rights {
    width: 16rpx;
    height: 30rpx;
    position: absolute;
    right: 0;
}

.rows view {
    font-size: 26rpx;
    color: #333333;
    margin-left: 30rpx;
    width: 120rpx;
}

.rows text {
    color: #999999;
    width: 60%;


    font-size: 26rpx;
}

.order-rule {
    width: 92%;
    margin: auto;
    background: #fff;
    padding-bottom: 0rpx;
    margin-top: 30rpx;
    border-radius: 10rpx;
}
.share{
    width: 140rpx;
    height: 140rpx;
    position: fixed;
    top: 50%;
    right: 40rpx;
}
.share image{
    position: absolute;
    width: 100%;
    height: 100%;
}
.order-border {
    display: flex;
    align-items: center;
    position: relative;
    top: 30rpx;
    color: #333333;
    font-size: 32rpx;
    font-weight: 600;

}
 button {
    background-color: transparent;
    color: #333333;
    font-weight: normal;
    font-size: 30rpx;
    padding: 0;
    box-sizing: border-box;
    width: 100%;

    text-align: left;
}


 button::after {
    border: none;
    background-color: transparent !important;
}

 button-hover {
    background: #007D58 !important;
}
.order-border image {
    width: 8rpx;
    margin-right: 20rpx;
}
.qr-code{
    width: 90%;
    height: 500rpx;
    display: flex;
    margin: auto;
}
.guanzhu{
    text-align: center;
    margin-top: 10rpx;
}
.border {
    width: 94%;
    margin: auto;
    height: 340rpx;
    /* border: 1px solid rgba(152, 46, 223, 0.19); */
    margin-top: 50rpx;
    border-radius: 20rpx;
    overflow-y: scroll;
    word-break: break-all;
    font-size: 24rpx;
    color: #333333;
}

.border view {
    margin: 10rpx;
    margin-left: 20rpx;
    white-space: pre-wrap;
}


.order-swp {
    text-align: center;
    display: flex;
    margin-left: 20rpx;
    margin-top: 30rpx;
    align-items: center;
    color: #999999;
    font-size: 26rpx;
    padding-bottom: 20rpx;
}

.order-sign {
    width: 92%;
    height: 90rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 80rpx;
    margin: auto;
    margin-top: 80rpx;
    margin-bottom: 140rpx;
    font-size: 34rpx;
    color: #FFFFFF;
    background: linear-gradient(180deg, #9F00B6 0%, #FF4BBD 100%);
}

.move {
    position: fixed;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.9);
    z-index: 11;
    display: flex;
}

.move .block {
    width: 70%;
    margin: auto;
    height: 400rpx;
    background-color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    color: #26B2A9;
    font-weight: bold;
    border-radius: 20rpx;
    position: relative;
}

.block image {
    width: 50%;
   
}
.block .close{
    position: absolute;
    bottom: -120rpx;
    width: 80rpx;
}
.conme{
    width: 90%;
    margin: auto;
    height: 100rpx;
    display: flex;
    align-items: center;
    border-bottom: 1px solid #eeeeee;
}
.shenqing{
    width: 140rpx !important;
    height: 60rpx !important;
    position: absolute;
    right: 0;
    background: linear-gradient(180deg, #9F00B6 0%, #FF4BBD 100%);
    color: #fff !important;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50rpx;

}

.van-share-sheet__name {
    color: #646566;
    margin: 0 12px;
    margin-top: 8px;
}
.move-row{
    display: flex;
    flex-direction: column;
   
}
.move-row view{
    margin-top: 5rpx;
}
.move-title{
    color: #999999;
    font-size: 24rpx;
}
.move-title text{
    color: #982EDF;
}